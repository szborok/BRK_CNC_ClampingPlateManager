{
  "name": "clamping-plate-manager",
  "version": "1.0.0",
  "description": "CNC Clamping Plate Management Backend Service - Node.js API for plate inventory tracking, work order management, and manufacturing workflow integration.",
  "private": true,
  "author": "CNC Manufacturing Team",
  "license": "Private",
  "keywords": [
    "cnc",
    "manufacturing",
    "inventory",
    "plates",
    "nodejs",
    "backend",
    "api"
  ],
  "main": "main.js",
  "scripts": {
    "setup-test-data": "node scripts/setup-test-data.js",
    "start": "node main.js --serve",
    "serve": "node main.js --serve",
    "init-excel": "node main.js --init-excel",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "node scripts/setup-test-data.js && node main.js --test",
    "test:no-setup": "node main.js --test"
  },
  "dependencies": {
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "mongodb": "^6.20.0",
    "uuid": "^11.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^8.57.1",
    "jest": "^30.2.0",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
